<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFF66">

    <TextView
        android:id="@+id/titleCatSounds"
        android:layout_width="712dp"
        android:layout_height="0dp"
        android:layout_marginStart="49dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="19dp"
        android:fontFamily="@font/quicksand_medium"
        android:text="The Sounds of Cats"
        android:textColor="@color/purple_777"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="534dp"
        android:layout_height="131dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/cartooncatcute" />

    <Button
        android:id="@+id/soundOneCat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:fontFamily="@font/quicksand_medium"
        android:text="Meow"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="@+id/titleCatSounds"
        app:layout_constraintTop_toTopOf="@+id/imageView9" />

    <Button
        android:id="@+id/soundTwoCat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="89dp"
        android:layout_marginTop="14dp"
        android:fontFamily="@font/quicksand_medium"
        android:text="Hungry"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView9" />

    <Button
        android:id="@+id/soundThreeCat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="Purr"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/soundTwoCat"
        app:layout_constraintStart_toStartOf="@+id/soundTwoCat" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="33dp"
        android:fontFamily="@font/quicksand_medium"
        android:onClick="loadHomePageCats"
        android:text="Home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>